<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="text" format="string" />
        <attr name="hint" format="string" />
        <attr name="form_type" format="string" />
        <attr name="inputType" format="integer" />
        <attr name="isSecure" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomProgressBar">
        <attr name="pd_progress" format="integer" />
        <attr name="pd_text" format="string" />
        <attr name="pd_description" format="string" />
        <attr name="pd_custom_background_drawable" format="integer" />
    </declare-styleable>
</resources>